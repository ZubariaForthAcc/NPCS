SELECT ?var3 (CONCAT("⊕(",GROUP_CONCAT(?fprov) ,")")  AS ?finalprovennacevariable) 
WHERE { 
	?var1 <http://www.wikidata.org/prop/P18> ?fprov0 . 
	?fprov0 <http://www.wikidata.org/prop/statement/P18> ?var3 . 
	?var1 <http://www.wikidata.org/prop/P31> ?fprov1 . 
	?fprov1 <http://www.wikidata.org/prop/statement/P31> ?var4 . 
	BIND (CONCAT("(⊗",STR(?fprov0) ,",",STR(?fprov1) ,",",")")  AS ?fprov) . 
 }
 GROUP BY ?var3