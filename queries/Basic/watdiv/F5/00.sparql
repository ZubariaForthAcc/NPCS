PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX gr: <http://purl.org/goodrelations/>
PREFIX og: <http://ogp.me/ns#>
SELECT ?v0 ?v1 ?v3 ?v4 ?v5 ?v6 WHERE {
	?v0	gr:includes	?v1 .
	?v2	gr:offers	?v0 .
	?v0	gr:price	?v3 .
	?v0	gr:validThrough	?v4 .
	?v1	og:title	?v5 .
	?v1	rdf:type	?v6 .
}
